package nopcommerceV001_cucumber;

public class Person {
	int age;
	String name;

	
	

public Person(String name,int age)
{
	this.name = name;
	this.age = age;
	
}


public String getName() {
	return name;
}

public int getAge() {
	return age;
}
	
public static void main(String[] a) {
	Person p1 = new Person("nyn", 37);
	Person p2 = new Person("kir", 35);
	p1.getName();
	System.out.println(p1.getName());
	p1.getAge();
	p2.getName();
	p2.getAge();
	
}

}
